  <builders>
    <hudson.tasks.Shell>
      <command>#!/bin/sh
#git checkout $branch
cd nectar-openapi-common/nectar-openapi-common/
docker rmi -f common_build
docker build -t common_build .
docker run -v `pwd`:/usr/src/mymaven common_build mvn clean -s settings.xml deploy org.codehaus.mojo:sonar-maven-plugin:3.3.0.603:sonar -Dsonar.scm.disabled=true -Dsonar.host.url=http://galsonar:9000 -Dsonar.projectKey=&quot;org.sonarqube:banking-common-service&quot; -Dsonar.projectName=&quot;banking-common-service&quot; -Dsonar.branch=&quot;${branch}&quot; 
echo &apos;completed common build ..&apos;

echo &apos;preparing for common properties build ..&apos;
cd ../..
cd nectar-openapi-properties
docker run -v `pwd`:/usr/src/mymaven common_build mvn clean -s settings.xml deploy
echo &apos;common properties build completed ...&apos;
</command>
    </hudson.tasks.Shell>
